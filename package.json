{
    "name": "@veebisepad/vue-query-filters",
    "version": "3.3.3",
    "description": "Reactive utility for managing URL query parameters in Vue 3.",
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/veebisepad/vue-query-filters.git"
    },
    "keywords": [
        "vue",
        "3",
        "queryparams",
        "search",
        "filter",
        "vue3",
        "laravel",
        "inertia",
        "inertiajs",
        "spatie"
    ],
    "author": "ralf@veebisepad",
    "license": "MIT",
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "tsup": "^8.5.1",
        "typescript": "^5.9.3",
        "vue": "^3.3.4"
    }
}
