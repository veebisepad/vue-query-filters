        "prepublishOnly": "npm run clean && npm run build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/veebisepad/vue-query-filters.git"
    },
    "keywords": [
        "vue",
        "3",
        "queryparams",
        "search",
        "filter",
        "vue3",
        "laravel",
        "inertia",
        "inertiajs",
        "spatie"
    ],
    "author": "ralf@veebisepad",
    "license": "MIT",
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "typescript": "^4.9.5",
        "vue": "^3.3.4"
    }
}
